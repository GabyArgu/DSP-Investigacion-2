{
  "expo": {
    "name": "investigacion_aplicada_2",
    "slug": "investigacion_aplicada_2",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "investigacionaplicada2",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "Esta aplicación utiliza la cámara para mostrar datos en realidad aumentada.",
        "NSLocationWhenInUseUsageDescription": "Esta aplicación utiliza la ubicación para mostrar sensores cercanos.",
        "NSMicrophoneUsageDescription": "Esta app no requiere acceso al micrófono."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "CAMERA",
        "INTERNET",
        "ACCESS_NETWORK_STATE",
        "ACCESS_WIFI_STATE",
        "VIBRATE"
      ],
      "package": "com.gabmendez4869.investigacion_aplicada_2"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "@reactvision/react-viro"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "0988cfdb-901a-4578-b54b-1f5819dc5793"
      }
    },
    "owner": "gabmendez4869"
  }
}
